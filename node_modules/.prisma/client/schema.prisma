generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int    @id @default(autoincrement())
  name     String
  email    String @unique
  password String
}

model agendamento {
  id         Int      @id @default(autoincrement())
  data       DateTime
  horario    DateTime
  curso      String
  sala       String
  disciplina String
  tipoProva  String
  suporte    String
}
